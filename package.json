{
  "name": "inversify-react",
  "version": "0.3.0",
  "description": "Components and decorators to connect react with inversify.",
  "main": "./dist/index.js",
  "typings": "./types/index.d.ts",
  "scripts": {
    "init": "npm install",
    "dev": "webpack --mode development -w",
    "prod": "webpack --mode production",
    "prepublishOnly": "npm run prod",
    "test": "jest --no-cache"
  },
  "author": "Lukáš Novotný",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kukkimonsuta/inversify-react.git"
  },
  "keywords": [
    "inversify",
    "react"
  ],
  "dependencies": {
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/prop-types": "^15.5.6",
    "@types/react": "^16.4.18",
    "@types/react-test-renderer": "^16.0.3",
    "inversify": "^5.0.1",
    "jest": "^23.6.0",
    "react": "^16.6.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.6.1",
    "react-test-renderer": "^16.6.1",
    "reflect-metadata": "^0.1.12",
    "ts-loader": "^5.3.0",
    "typescript": "^3.1.6",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "inversify": "^3.0.0 || ^4.0.0 || ^5.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/test/framework/preprocessor.js"
    },
    "testRegex": "/test/.*\\.(ts|tsx)$"
  }
}
