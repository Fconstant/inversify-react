{
  "name": "inversify-react",
  "version": "0.1.4",
  "description": "Components and decorators to connect react with inversify.",
  "main": "./dist/index.js",
  "typings": "./types/index.d.ts",
  "scripts": {
    "test": "jest --no-cache",
    "dev": "webpack -d -w",
    "prod": "webpack -p",
    "prepublish": "npm run prod"
  },
  "author": "Lukáš Novotný",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kukkimonsuta/inversify-react.git"
  },
  "keywords": [
    "inversify",
    "react"
  ],
  "devDependencies": {
    "@types/jest": "^18.1.1",
    "@types/react": "^15.0.12",
    "@types/react-test-renderer": "^15.4.2",
    "jest": "^19.0.2",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "reflect-metadata": "^0.1.10",
    "ts-loader": "^2.0.1",
    "typescript": "^2.2.1",
    "webpack": "^2.2.1"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0-alpha",
    "inversify": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/test/framework/preprocessor.js"
    },
    "testRegex": "/test/.*\\.(ts|tsx)$"
  }
}
