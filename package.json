{
  "name": "inversify-react",
  "version": "0.2.5",
  "description": "Components and decorators to connect react with inversify.",
  "main": "./dist/index.js",
  "typings": "./types/index.d.ts",
  "scripts": {
    "init": "npm install",
    "dev": "webpack --mode development -w",
    "prod": "webpack --mode production",
    "prepublishOnly": "npm run prod",
    "test": "jest --no-cache"
  },
  "author": "Lukáš Novotný",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kukkimonsuta/inversify-react.git"
  },
  "keywords": [
    "inversify",
    "react"
  ],
  "dependencies": {
    "prop-types": "^15.6.1"
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.41",
    "@types/react-test-renderer": "^16.0.1",
    "inversify": "^4.11.1",
    "jest": "^22.4.3",
    "react": "^16.2.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "reflect-metadata": "^0.1.12",
    "ts-loader": "^4.1.0",
    "typescript": "^2.7.2",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "inversify": "^3.0.0 || ^4.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/test/framework/preprocessor.js"
    },
    "testRegex": "/test/.*\\.(ts|tsx)$"
  }
}
