{
  "name": "inversify-react",
  "version": "0.1.3",
  "description": "Components and decorators to connect react with inversify.",
  "main": "./dist/index.js",
  "typings": "./types/index.d.ts",
  "scripts": {
    "test": "jest --no-cache",
    "dev": "webpack -d -w",
    "prod": "webpack -p",
    "prepublish": "npm run prod"
  },
  "author": "Lukáš Novotný",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kukkimonsuta/inversify-react.git"
  },
  "keywords": [
    "inversify",
    "react"
  ],
  "devDependencies": {
    "@types/jest": "^16.0.2",
    "@types/react": "^0.14.55",
    "@types/react-test-renderer": "^15.4.0",
    "jest": "^17.0.0",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-test-renderer": "^15.4.1",
    "reflect-metadata": "^0.1.8",
    "ts-loader": "^1.3.3",
    "typescript": "^2.1.4",
    "webpack": "^2.2.0-rc.2"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0-alpha",
    "inversify": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/test/framework/preprocessor.js"
    },
    "testRegex": "/test/.*\\.(ts|tsx)$"
  }
}
